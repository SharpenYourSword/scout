<% items.each do |item| %>
  <item>

    <%= partial "subscriptions/#{item.subscription_type}/rss", engine: "erb", locals: {item: item, interest: (defined?(interest) ? interest : item.interest.extra!)} %>

    <link><%= item_url item %></link>
    <guid><%= item_guid item %></guid>
    <pubDate><%= rss_date item.date %></pubDate>
  </item>
<% end %>