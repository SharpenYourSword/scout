
  <% if items.any? %>

    <ul class="items">
      <% current_date = nil %>
      <% items.each do |item| %>
      
        <li class="result <%= item.subscription.subscription_type %>">
          <%= partial :"subscriptions/#{item.subscription.subscription_type}/result", :locals => {:item => item, :keyword => keyword} %>
        </li>
        
      <% end %>
    </ul>
    
  <% else %>
    <p class="none atAll">
        No results found for this search.
    </p>
  <% end %>
