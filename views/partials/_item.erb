<li class="result <%= item.subscription_type %>">
  <div class="left">
    <p class="date">
      <%= just_date_year item.date %>
    </p>
  </div>
  <div class="right">
    <%= partial "subscriptions/#{item.subscription.subscription_type}/result", :engine => "erb", :locals => {:item => item, :interest => interest_in} %>

    <section class="more">
      <a class="landing" href="<%= item_path item %>">
        More information &raquo;
      </a>
    </section>
  </div>
</li>