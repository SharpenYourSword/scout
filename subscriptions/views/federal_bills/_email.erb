<% bill = item.data %>
<%= bill_code bill['bill_type'], bill['number'] %> - <% if bill['short_title'].present? %>
<%= bill['short_title'] %>
<% else %>
<%= bill['official_title'] %>
<% end %>

 <% if item.data['search']['highlight'] %>
<%= bill_highlight item, interest.in, false %>
 <% end %>

<%= item_url item %>