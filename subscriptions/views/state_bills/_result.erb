<% bill = item.data %>
<h2>
  <%= bill['bill_id'] %> from <span class="state"><%= state_name bill['state'] %></span>
</h2>

<p class="content">
  <%= excerpt bill['title'], interest %>
</p>

<section class="more">
    <a class="landing" href="<%= item_path item %>">
        More information &raquo;
    </a>
</section>