<% bill = item.data %>
[<%= state_name bill['state'] %>] <%= excerpt bill['title'], interest.in, false %>

<% if state_version_info?(bill) %>
<%= URI.encode bill['versions'].last['url'] %>
<% elsif state_source_info?(bill) %>
<% bill['sources'].each do |source| %>
<%= URI.encode source['url'] %>
<% end %>
<% end %>