<% bill = item.data %>
<item>
  <title>
    <%= state_name bill['state'] %> - <%= bill['bill_id'] %>
  </title>
  <description><![CDATA[
    <%= excerpt bill['title'], keyword %>
  ]]></description>
    
  <link><%= rss_encode bill['versions'].last['url'] %></link>
  <pubDate><%= rss_date item.date %></pubDate>
</item>