<% bill = item.data %>
<item>
  <title>
    <%= state_name bill['state'] %> - <%= bill['bill_id'] %>
  </title>
  <description><![CDATA[
    <%= excerpt bill['title'], interest.in %>
  ]]></description>
    
  <link><%= item_path item %></link>
  <guid><%= item_path item %></guid>
  <pubDate><%= rss_date item.date %></pubDate>
</item>